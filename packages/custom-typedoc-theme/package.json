{
  "name": "custom-typedoc-theme",
  "description": "Default themes for TypeDoc.",
  "version": "1.0.0-beta.3",
  "homepage": "http://typedoc.org/",
  "private": true,
  "main": "bin/plugin.js",
  "files": [
    "bin",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rimraf bin",
    "grunt": "node ./node_modules/grunt/bin/grunt",
    "build": "yarn run clean && yarn run grunt",
    "nohoist": "mv ../../node_modules/grunt* ./node_modules",
    "prepublish": "yarn run build"
  },
  "author": {
    "name": "Sebastian Lenz",
    "url": "http://www.sebastian-lenz.de/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/sebastian-lenz/typedoc-default-themes.git"
  },
  "bugs": {
    "url": "https://github.com/sebastian-lenz/TypeDoc/issues"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">= 4"
  },
  "devDependencies": {
    "grunt": "^1.0.1",
    "grunt-autoprefixer": "^3.0.4",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-sass": "^1.0.0",
    "grunt-contrib-uglify": "^2.3.0",
    "grunt-contrib-watch": "~1.0.0",
    "grunt-string-replace": "^1.0.0",
    "grunt-ts": "^6.0.0-beta.17",
    "rimraf": "^2.6.1",
    "typedoc": "^0.9.0",
    "typescript": "2.4.2"
  },
  "keywords": [
    "typescript",
    "documentation",
    "generator"
  ],
  "workspaces": {
    "nohoist": [
      "grunt*"
    ]
  }
}
